<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event category="COLLISION" with="PlayerWall">
  <actions>
    <action id="603" library="1">
      <!--action name: Code-->
      <kind>CODE</kind>
      <allowRelative>false</allowRelative>
      <question>false</question>
      <canApplyTo>true</canApplyTo>
      <actionType>CODE</actionType>
      <functionName/>
      <relative>false</relative>
      <not>false</not>
      <appliesTo>.self</appliesTo>
      <arguments>
        <argument kind="STRING">    x=xprevious;
    hspeed=0;
    if(place_meeting(x,y,PlayerWall)) {
        if(not place_meeting(x+8,y,PlayerWall)) {
            move_contact_solid(0,8);
        } else if(not place_meeting(x-8,y,PlayerWall)) {
            move_contact_solid(180,8);
        }
    }</argument>
      </arguments>
    </action>
  </actions>
</event>
